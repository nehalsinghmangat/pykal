% BibTeX file for pykal community contributions
%
% Custom fields used for filtering:
%   - keywords: algorithm category - must be "estimation", "control", or "other"
%   - impl_pykal: path to notebook if implemented in pykal core (e.g., "notebooks/community_library/my_algorithm_pykal.ipynb")
%   - impl_turtlebot: path to notebook if implemented for turtlebot (e.g., "notebooks/community_library/my_algorithm_turtlebot.ipynb")
%   - impl_crazyflie: path to notebook if implemented for crazyflie (e.g., "notebooks/community_library/my_algorithm_crazyflie.ipynb")
%   - note: Brief description of what the notebook does (will be displayed in red)
%
% Template for new entries:
%
% @misc{unique_citation_key,
%   title        = {Title of Your Notebook},
%   author       = {Your Name},
%   year         = {2025},
%   keywords     = {estimation},  % or "control" or "other"
%   impl_pykal   = {notebooks/community_library/your_notebook_pykal.ipynb},
%   impl_turtlebot = {notebooks/community_library/your_notebook_turtlebot.ipynb},
%   impl_crazyflie = {notebooks/community_library/your_notebook_crazyflie.ipynb},
%   note         = {Brief description of what your notebook demonstrates}
% }

% Example community contribution (template - delete when adding real contributions)
@misc{example:custom_controller,
  title        = {Custom Adaptive Controller for Mobile Robots},
  author       = {Community Contributor},
  year         = {2025},
  keywords     = {control},
  impl_pykal   = {notebooks/community_library/adaptive_controller_pykal.ipynb},
  impl_turtlebot = {notebooks/community_library/adaptive_controller_turtlebot.ipynb},
  note         = {Example notebook demonstrating adaptive control techniques for differential drive robots}
}
